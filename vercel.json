{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.2"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images|fonts)/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/public/$1/$2"
        },
        {
            "src": "/plugins/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/public/plugins/$1"
        },
        {
            "src": "/dist/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/public/dist/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*\\.(jpg|jpeg|png|gif|svg|ico|webp|woff|woff2|ttf|eot))",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://tusome-iota.vercel.app",
        "ASSET_URL": "https://tusome-iota.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "SESSION_SECURE_COOKIE": "true"
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=31536000; includeSubDomains"
                }
            ]
        }
    ]
}